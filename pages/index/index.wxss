page{
  background-color: #381924;
}

slider{
  margin: 0 auto;
  width: 76vw;
}

.panel{
  display: flex;
  align-items: center;
  justify-content: center;
}

.sliders_container{
  background-color: #381924;
  height: 13vw;
  display: flex;
  align-items: center;
  justify-content: center;
}

.slider_and_intro{
  width: 100vw;
  display: flex;
  align-items: center;
  justify-content: center;
}

.intro_button{
  background-color: #ffe75e;
  width: 14vw;
  height: 9vw;
  margin-top: 4vw;
  border-radius: 5vw 5vw 0 0;
}

.intro_panel{
  background-color: #fcff9d;
  margin: 1vw;
  margin-bottom: 0;
  /* margin-top: 2vw; */
  width: 12vw;
  height: 8vw;
  border-radius: 5vw;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #82860e;
  font-weight: bold;
  font-size: 4vw;
}

.slider_box_left{
  background-color: #381924;
  height: 13vw;
  width: 4vw;
}

.slider_box_right{
  background-color: #381924;
  height: 13vw;
  width: 4vw;
  border-bottom-right-radius: 4vw;
}

.slider_box{
  background-color: #ffe75e;
  display: flex;
  align-items: center;  
  justify-content: center;
}

.choose_box{
  background-color: #381924;
  display: flex;
  align-items: center;
  justify-content: center;
}

.choose_panel_frames{
  background-color: #ffe75e;
  height: 17.5vw;
  width: 94vw;
  border-radius: 4vw 0 4vw 4vw;
  display: flex;
  align-items: center;
  justify-content: center;
}

.qr_item{
  background-color: #fcff9d;
  height: 15vw;
  width: 15vw;
  margin: 1vw;
  margin-right: 0;
  border-radius: 4vw;
  display: flex;
  align-items: center;
  justify-content: center;
}

.choose_item{
  height: 15.5vw;
  width: 15.5vw;
  margin-left: 1vw;
  margin-right: 1vw;
}

.frames_box{
  /* background-color: #ffe75e; */
  height: 16vw;
  width: 76vw;
  margin-right: 1vw;
  white-space: nowrap;
}

.menu_box{
  background-color: #381924;
  height: 19vw;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 4vw;
  font-weight: bold;
}

.menu_selected_mark{
  position: absolute;
  background-color: #ffe75e;
  height: 1vw;
  display: flex;
  align-items: center;
  justify-content: center;
}

.menu_selected_mark_left{
  width: 4vw;
  height: 1vw;
  border-radius: 0 1vw 0 0;
  background-color: #381924;
}

.menu_selected_mark_center{
  width: 2vw;
  height: 3vw;
  border-radius: 0 0 1vw 1vw;
  background-color: #ffe75e;
}

.menu_selected_mark_right{
  width: 4vw;
  height: 1vw;
  border-radius: 1vw 0 0 0;
  background-color: #381924;
}

.menu_item{
  background-color: #381924;
  color: #fffef8;
  height: 19vw;
  width: 20vw;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.icon{
  width: 10vw;
  height: 10vw;
  margin-top: 1vw;
  background-size: cover;
}

