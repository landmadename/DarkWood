page{
  background-color: #381924;
}

movable-view{
  width: 60px;
  height: 60px;
}

slider{
  margin: 0 auto;
  width: 76vw;
}

.panel_area{
  display: flex;
  align-items: center;
  justify-content: center;
}

.sliders_container{
  background-color: #381924;
  height: 13vw;
  display: flex;
  align-items: center;
  justify-content: center;
}

.slider_and_intro_button{
  width: 100vw;
  display: flex;
  align-items: center;
  justify-content: center;
}

.intro_button{
  background-color: #ffe75e;
  width: 14vw;
  height: 9vw;
  margin-top: 4vw;
  border-radius: 5vw 5vw 0 0;
}

.intro_box{
  background-color: #fcff9d;
  margin: 1vw;
  margin-bottom: 0;
  width: 12vw;
  height: 8vw;
  border-radius: 5vw;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #82860e;
  font-weight: bold;
  font-size: 4vw;
}

.slider{
  background-color: #ffe75e;
  display: flex;
  align-items: center;  
  justify-content: center;
}

.slider:after{
  content: " ";
  background-color: #381924;
  height: 13vw;
  width: 4vw;
  border-bottom-right-radius: 4vw;
}


/* choose area */

.choose_area{
  background-color: #381924;
  display: flex;
  align-items: center;
  justify-content: center;
}

.choose_panel{
  background-color: #ffe75e;
  height: 17vw;
  width: 94vw;
  border-radius: 4vw 0 4vw 4vw;
  display: flex;
  align-items: center;
  justify-content: space-around; 
}

.panel_button, .small_panel_button{
  background-color: #fcff9d;
  height: 15vw;
  width: 15vw;
  margin: 0vw 1vw 0vw 1vw;
  /* margin-right: 0; */
  border-radius: 4vw;
  display: flex;
  align-items: center;
  justify-content: center;
}

.small_panel_button{
  display: inline-block;
  height: 12vw;
  width: 12vw;
  margin: 3vw 1vw 3vw 1vw;
}

.choice_img, .choice_box{
  height: 15vw;
  width: 15vw;
  margin: 1vw;
  display: inline-block;
  border-radius: 2vw;
}

.choice_img{
  margin: 0vw;
}

.chosen_mark[data-choosen="true"]{
  margin-left: 2vw;
  margin-top: 1vw;
  height: 4vw;
  width: 4vw;
  position: absolute;
  background-color: #fcff9d;
  border-radius: 4vw;
}

.choices_container{
  height: 17vw;
  width: 76vw;
  margin-right: 1vw;
  white-space: nowrap;
}

/* menu area*/

.menu_area{
  background-color: #381924;
  height: 20vw;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 4vw;
  font-weight: bold;
}

.menu_selected_mark{
  position: absolute;
  background-color: #ffe75e;
  height: 1vw;
  display: flex;
  align-items: center;
  justify-content: center;
}

.menu_selected_mark_left{
  width: 4vw;
  height: 1vw;
  border-radius: 0 1vw 0 0;
  background-color: #381924;
}

.menu_selected_mark_center{
  width: 2vw;
  height: 3vw;
  border-radius: 0 0 1vw 1vw;
  background-color: #ffe75e;
}

.menu_selected_mark_right{
  width: 4vw;
  height: 1vw;
  border-radius: 1vw 0 0 0;
  background-color: #381924;
}

.menu_item{
  background-color: #381924;
  color: #fffef8;
  height: 20vw;
  width: 20vw;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

